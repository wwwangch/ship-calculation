//插入一个坐标，4个点
INSERT INTO req (id,area) VALUES ('wgweg111', ST_GeomFromText('POLYGON((0 0,10 0,10 10,0 10))'));

//插入一个坐标，一个点
INSERT INTO req (id,area) VALUES ('wgweg1wee11', ST_PointFromText('POINT(1 1)'));