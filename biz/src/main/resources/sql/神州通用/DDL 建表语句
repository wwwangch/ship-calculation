CREATE TABLE SYSDBA.CATALOG
(
    ID integer NOT NULL AUTO_INCREMENT,
    "NUMBER" character varying(100) ,
    PID integer ,
    "NAME" character varying(100) , 
    CONSTRAINT CATALOG_PK PRIMARY KEY (ID)
)  
 BINLOG ON ;
--Indexes DDL
CREATE UNIQUE INDEX CATALOG_PK ON SYSDBA."CATALOG" USING BTREE(ID) TABLESPACE SYSTEM INIT 64K NEXT 64K MAXSIZE UNLIMITED FILL 70 SPLIT 50;
CREATE TABLE SYSDBA.CATALOGUE
(
    ID integer NOT NULL AUTO_INCREMENT,
    LEVEL1 character varying(255) ,
    LEVEL2 character varying(255) ,
    LEVEL3 character varying(255) ,
    NUM character varying(100) ,
    PART character varying(100) , 
    CONSTRAINT TBL_CATALAGUE_PKEY PRIMARY KEY (ID)
)  
 BINLOG ON ;
--Indexes DDL
CREATE UNIQUE INDEX TBL_CATALAGUE_PKEY ON SYSDBA.CATALOGUE USING BTREE(ID) TABLESPACE SYSTEM INIT 64K NEXT 64K MAXSIZE UNLIMITED FILL 70 SPLIT 50;
CREATE TABLE SYSDBA.DICT_DATA
(
    ID integer NOT NULL AUTO_INCREMENT,
    DICT_TYPE character varying(11) DEFAULT '业务类' ,
    DICT_DATA_TYPE character varying(50) DEFAULT NULL ,
