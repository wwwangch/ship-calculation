    DICT_DESC character varying(255) DEFAULT NULL ,
    CREATE_TIME character varying(20) DEFAULT NULL ,
    CREATE_BY character varying(20) DEFAULT NULL ,
    UPDATE_TIME character varying(20) DEFAULT NULL ,
    UPDATE_BY character varying(20) DEFAULT NULL , 
    CONSTRAINT DICT_DATA_PKEY PRIMARY KEY (ID)
)  
 BINLOG ON ;
--Indexes DDL
CREATE UNIQUE INDEX DICT_DATA_PKEY ON SYSDBA.DICT_DATA USING BTREE(ID) TABLESPACE SYSTEM INIT 64K NEXT 64K MAXSIZE UNLIMITED FILL 70 SPLIT 50;
