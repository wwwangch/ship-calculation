    CONSTRAINT LOG_INFO_PKEY PRIMARY KEY (ID)
)  
 BINLOG ON ;
--Indexes DDL
CREATE UNIQUE INDEX LOG_INFO_PKEY ON SYSDBA.LOG_INFO USING BTREE(ID) TABLESPACE SYSTEM INIT 64K NEXT 64K MAXSIZE UNLIMITED FILL 70 SPLIT 50;
CREATE TABLE SYSDBA.MENU
(
    MENU_ID integer NOT NULL AUTO_INCREMENT,
    MENU_PID integer DEFAULT NULL ,
    MENU_PAGE character varying(255) NOT NULL,
    MENU_CREATE_TIME date DEFAULT SYSDATE() ,
    MENU_UPDATE_TIME date DEFAULT SYSDATE() ,
    MENU_NAME character varying(255) DEFAULT NULL , 
    CONSTRAINT MENU_PKEY PRIMARY KEY (MENU_ID), 
    CONSTRAINT MENU_MENU_NAME_KEY UNIQUE (MENU_NAME), 
    CONSTRAINT MENU_IBFK_1 FOREIGN KEY (MENU_PID) REFERENCES SYSDBA.MENU(MENU_ID)
)  
 BINLOG ON ;
--Indexes DDL
CREATE UNIQUE INDEX MENU_MENU_NAME_KEY ON SYSDBA.MENU USING BTREE(MENU_NAME) TABLESPACE SYSTEM INIT 64K NEXT 64K MAXSIZE UNLIMITED FILL 70 SPLIT 50;
CREATE UNIQUE INDEX MENU_PKEY ON SYSDBA.MENU USING BTREE(MENU_ID) TABLESPACE SYSTEM INIT 64K NEXT 64K MAXSIZE UNLIMITED FILL 70 SPLIT 50;
CREATE TABLE SYSDBA.MENU_OPRATION
(
    MENU_ID integer NOT NULL,
    OP_ID integer NOT NULL, 
    CONSTRAINT MENU_OPRATION_PKEY PRIMARY KEY (MENU_ID, OP_ID), 
    CONSTRAINT MENU_OPRATION_IBFK_1 FOREIGN KEY (MENU_ID) REFERENCES SYSDBA.MENU(MENU_ID), 
    CONSTRAINT MENU_OPRATION_IBFK_2 FOREIGN KEY (OP_ID) REFERENCES SYSDBA.OPRATION(OP_ID)
)  
 BINLOG ON ;
--Indexes DDL
CREATE UNIQUE INDEX MENU_OPRATION_PKEY ON SYSDBA.MENU_OPRATION USING BTREE(MENU_ID, OP_ID) TABLESPACE SYSTEM INIT 64K NEXT 64K MAXSIZE UNLIMITED FILL 70 SPLIT 50;
