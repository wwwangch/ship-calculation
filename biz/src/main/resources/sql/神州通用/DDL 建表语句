CREATE TABLE SYSDBA.XXCOLUMN_DEFINITION
(
    ID integer NOT NULL AUTO_INCREMENT,
    TABLEIDENTITY character varying(255) NOT NULL,
    FIELD character varying(255) DEFAULT NULL ,
    "HEADER" character varying(255) DEFAULT NULL ,
    "SEQUENCE" integer DEFAULT NULL ,
    EDITABLE bit(1) DEFAULT B'0'::"BIT" ,
    SORTABLE bit(1) DEFAULT NULL ,
    "TYPE" character(16) DEFAULT NULL ,
    SELECTURL character varying(255) DEFAULT NULL ,
    SEARCH bit(1) DEFAULT B'0'::"BIT" ,
    SEARCHTYPE character(8) DEFAULT NULL ,
    LINK bit(1) DEFAULT NULL ,
    ADDABLE bit(1) DEFAULT B'0'::"BIT" ,
    HIDDEN bit(1) DEFAULT B'0'::"BIT" ,
    REFTABLE character varying(255) DEFAULT NULL ,
    REFCOLUMN character varying(255) DEFAULT NULL ,
    REFVALUE character varying(512) DEFAULT NULL ,
    REQUIRED bit(1) DEFAULT B'0'::"BIT" ,
    REG character varying(255) DEFAULT NULL ,
    MINLENGTH integer DEFAULT NULL ,
    MAXLENGTH integer DEFAULT NULL ,
    "DISTINCT" bit(1) DEFAULT NULL , 
    CONSTRAINT XXCOLUMN_DEFINITION_PKEY PRIMARY KEY (ID)
)  
 BINLOG ON ;
--Indexes DDL
CREATE UNIQUE INDEX XXCOLUMN_DEFINITION_PKEY ON SYSDBA.XXCOLUMN_DEFINITION USING BTREE(ID) TABLESPACE SYSTEM INIT 64K NEXT 64K MAXSIZE UNLIMITED FILL 70 SPLIT 50;
CREATE TABLE SYSDBA.XXTABLE_DEFINITION
(
    ID integer NOT NULL AUTO_INCREMENT,
    TABLEIDENTITY character varying(64) DEFAULT NULL ,
    TITLE character varying(255) DEFAULT NULL ,
    TABLENAME character varying(255) DEFAULT NULL ,
    SQL character varying(1023) DEFAULT NULL ,
    CHECKBOX bit(1) DEFAULT NULL ,
    BACKINFO character varying(255) DEFAULT NULL ,
    FRONTINFO character varying(255) DEFAULT NULL ,
    VIEWTYPE character(8) DEFAULT NULL ,
    CELLEDITABLE bit(1) DEFAULT NULL ,
    PRIMARYKEY character varying(255) DEFAULT NULL , 
    CONSTRAINT XXTABLE_DEFINITION_PKEY PRIMARY KEY (ID)
)  
 BINLOG ON ;
--Indexes DDL
CREATE UNIQUE INDEX XXTABLE_DEFINITION_PKEY ON SYSDBA.XXTABLE_DEFINITION USING BTREE(ID) TABLESPACE SYSTEM INIT 64K NEXT 64K MAXSIZE UNLIMITED FILL 70 SPLIT 50;
