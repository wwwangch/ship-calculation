CREATE TABLE SYSDBA.USER_INFO
(
    USER_ID integer NOT NULL AUTO_INCREMENT,
    USER_NAME character varying(255) NOT NULL,
    USER_REAL_NAME character varying(255) DEFAULT NULL ,
    USER_PWD character varying(255) DEFAULT NULL ,
    USER_TEL character varying(255) DEFAULT NULL ,
    USER_EMAIL character varying(255) DEFAULT NULL ,
    USER_STATUS smallint DEFAULT '1' ,
    USER_CREATE_TIME date DEFAULT SYSDATE() ,
    USER_UPDATE_TIME date DEFAULT SYSDATE() , 
    CONSTRAINT USER_INFO_PKEY PRIMARY KEY (USER_ID), 
    CONSTRAINT USER_INFO_USER_NAME_KEY UNIQUE (USER_NAME), 
    CONSTRAINT USER_INFO_IBFK_1 FOREIGN KEY (USER_STATUS) REFERENCES SYSDBA.USER_STATUS(ID)
)  
 BINLOG ON ;
--Indexes DDL
CREATE UNIQUE INDEX USER_INFO_PKEY ON SYSDBA.USER_INFO USING BTREE(USER_ID) TABLESPACE SYSTEM INIT 64K NEXT 64K MAXSIZE UNLIMITED FILL 70 SPLIT 50;
CREATE UNIQUE INDEX USER_INFO_USER_NAME_KEY ON SYSDBA.USER_INFO USING BTREE(USER_NAME) TABLESPACE SYSTEM INIT 64K NEXT 64K MAXSIZE UNLIMITED FILL 70 SPLIT 50;
CREATE TABLE SYSDBA.USER_ROLE
(
    USER_ID integer NOT NULL,
    ROLE_ID integer NOT NULL, 
    CONSTRAINT USER_ROLE_PKEY PRIMARY KEY (USER_ID, ROLE_ID), 
    CONSTRAINT USER_ROLE_IBFK_1 FOREIGN KEY (USER_ID) REFERENCES SYSDBA.USER_INFO(USER_ID), 
    CONSTRAINT USER_ROLE_IBFK_2 FOREIGN KEY (ROLE_ID) REFERENCES SYSDBA."ROLE"(ROLE_ID)
)  
 BINLOG ON ;
--Indexes DDL
CREATE UNIQUE INDEX USER_ROLE_PKEY ON SYSDBA.USER_ROLE USING BTREE(USER_ID, ROLE_ID) TABLESPACE SYSTEM INIT 64K NEXT 64K MAXSIZE UNLIMITED FILL 70 SPLIT 50;
