(
    ID integer NOT NULL AUTO_INCREMENT,
    LOG_TYPE character varying(20) DEFAULT NULL ,
    REQUEST_URL character varying(50) DEFAULT NULL ,
    OPERATE_TYPE character varying(20) DEFAULT NULL ,
    OPERATE_USER character varying(50) DEFAULT NULL ,
    LOG_DESC character varying(255) DEFAULT NULL ,
    OPERATE_TIME character varying(20) DEFAULT NULL ,
